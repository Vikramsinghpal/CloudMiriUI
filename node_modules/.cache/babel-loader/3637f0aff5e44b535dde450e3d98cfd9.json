ht: 120px;\r\n        animation: var(--time-animation);\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        right: 0;\r\n        bottom: 0;\r\n        margin: auto;\r\n        z-index:2000;\r\n    }\r\n\r\n    @keyframes spin {\r\n        0%{ transform: rotate(0deg);}\r\n        100%{ transform: rotate(360deg);}\r\n\r\n    }\r\n\r\n</style>"],"sourceRoot":"node_modules/vue-ui-preloader/src/components/loaders"}]}