\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-8\">\r\n            <div class=\"leftPanel\">\r\n              <div class=\"servMain\">\r\n                <div class=\"contMain\">\r\n                  <legend>\r\n                    <span class=\"fa fa-globe\"></span> Our offices\r\n                  </legend>\r\n                   <div class=\"addPan\" v-for=\"officeInfo in offices\" :key=\"officeInfo.CountryId\">\r\n                    <div class=\"flagHead\">\r\n                      <!-- <input              \r\n                        name=\"CountryID\"\r\n                        id=\"hdnCountryID\"                    \r\n                      /> -->\r\n\r\n                      <span class=\"addTitle\">{{officeInfo.CountryName}}</span>&nbsp;\r\n                      <span class=\"addflag\">\r\n                           <img v-bind:src=\"path + officeInfo.CountryFlagImage\" alt=\"\"  > \r\n                      </span>\r\n                    </div>\r\n\r\n                    <div class=\"col-md-6\" v-for=\" locations in officesAddress\" :key=\"locations.LocationId\">\r\n                      <span v-if=\"officeInfo.CountryId == locations.Country\">\r\n                        <span class=\"cityName\">{{locations.City}}, {{locations.State}}</span>\r\n                        <p>\r\n                          {{locations.Address1}}\r\n                          <br />\r\n                          {{locations.Address2}}\r\n                          <br />\r\n                          Phone:\r\n                          {{locations.Phone}}\r\n                          <br />\r\n                          Fax: {{locations.Fax}}\r\n                        </p>\r\n                      </span>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"conTag\">\r\n                    For any further query you can email us at\r\n                    <a\r\n                      href=\"mailto:contact@supportmycloud.com\"\r\n                    >contact@supportmycloud.com</a>\r\n                  </div>\r\n                  <p>&nbsp;</p>\r\n                  <font color=\"red\">*</font>\r\n                  Our Delhi Office (Head Office) ISO certified.\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-4\">\r\n            <div class=\"subFormSupport\">\r\n              <legend>&nbsp;</legend>\r\n              <div class=\"rightPanel addPan\">\r\n                <h3 class=\"titleHead\">\r\n                  <span class=\"headTxtLight\">Submit Your</span> Request\r\n                </h3>\r\n                <form ref=\"contact\" @submit.prevent=\"handleSubmit\">\r\n                  <div class=\"col-sm-12\">\r\n                    <div class=\"form-group\">\r\n                      <label class=\"control-label text-white\">Name:</label>\r\n                      <input type=\"text\" class=\"form-control\" v-model=\"user.name\" v-validate=\"'required'\" id=\"name\" \r\n                      name=\"name\"  :class=\"{ 'is-invalid': submitted && errors.has('name') }\"/>\r\n                         <div v-if=\"submitted && errors.has('name')\" class=\"invalid-feedback\">{{ errors.first('name') }}</div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-sm-12\">\r\n                    <div class=\"form-group\">\r\n                      <label class=\"control-label text-white\">Company/Organisation:</label>\r\n                     <input type=\"text\" class=\"form-control\" v-model=\"user.company\" v-validate=\"'required'\" id=\"company\" \r\n                      name=\"company\"  :class=\"{ 'is-invalid': submitted && errors.has('company') }\"/>\r\n                         <div v-if=\"submitted && errors.has('company')\" class=\"invalid-feedback\">{{ errors.first('company') }}</div>\r\n                    </div>\r\n                  </div>\r\n     